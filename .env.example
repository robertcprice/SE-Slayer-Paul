# AI Trading Bot Environment Configuration
# Copy this file to .env and fill in your actual values

# Database Configuration
DATABASE_URL=postgresql://username:password@localhost:5432/trading_bot

# Alpaca Trading API (Use paper trading keys for safety)
ALPACA_API_KEY=your_alpaca_api_key_here
ALPACA_SECRET_KEY=your_alpaca_secret_key_here

# OpenAI API for AI Market Analysis
OPENAI_API_KEY=your_openai_api_key_here

# Optional: Set environment (development/production)
NODE_ENV=development

# Database connection details (for manual setup)
PGHOST=localhost
PGPORT=5432
PGUSER=your_postgres_username
PGPASSWORD=your_postgres_password
PGDATABASE=trading_bot